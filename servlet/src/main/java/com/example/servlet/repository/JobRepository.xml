<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.servlet.repository.JobRepository">
    <insert id="save" parameterType="com.example.servlet.entity.JobMessage">

         insert  into jobregister(jobName,jobConditionOne,jobConditionTwo,cpnName,jobPay) values (#{jobName},#{jobConditionOne},#{jobConditionTwo},#{cpnName},#{jobPay})


    </insert>
</mapper>